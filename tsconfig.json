{
    "compilerOptions": {
      "target": "ESNext",
      "module": "ESNext",
      "lib": ["ESNext", "DOM"],
      "moduleResolution": "Node",
      "strict": true,
      "resolveJsonModule": true,
      "esModuleInterop": true,
      "noEmit": true,
      "skipDefaultLibCheck": true
    },
    "include": ["src"]
  }